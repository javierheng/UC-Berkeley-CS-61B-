public class Main {
    public static void main(String[] args) {
        int x = 10;
        int num ;

        while (x > 0) {
            num = x;
            while (num > 0){
            System.out.print("*");
            num = num - 1;
            }
            System.out.print("\n");
            x = x - 1;
        }

    }
}

